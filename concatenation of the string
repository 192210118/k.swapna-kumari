#include <stdio.h>  
#include <string.h>  
int main()
{  
  char string_1[25];
  char string_2[25];
  printf("Enter value for string_1 : "); 
  scanf("%s", string_1); 
  printf("\nEnter value for string_2 : ");
  scanf("%s",string_2);   
  strcat(string_1,string_2); 
  printf("After concatenation is done, the new concatenated string will be: %s",string_1);  
  return 0;  
}  
